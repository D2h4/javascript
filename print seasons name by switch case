let number=prompt("enter the number?");
switch(number)
{
  case '11':
  case '12':
  case '1':
  case '2':
     console.log("winter");
	     break;
  case '3':
  case '4':
  case '5':
  case '6':
    console.log("summer");
       break;
   case '7':
   case '8':
   case '9':  
   case '10':
    console.log("rainy");
	     break;
  default:
   console.log("sorry")  
}
